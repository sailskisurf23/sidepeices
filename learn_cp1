number1 = int(input('Please enter a number: '))
number2 = int(input('Please enter another number: '))

def compare(a,b):
    if a > b:
        c = 'The first number is larger.'
        return c
    elif b > a:
        c = 'The second number is larger.'
        return c
    else:
        c = 'The two numbers are equal.'
        return c

print(compare(number1,number2))
